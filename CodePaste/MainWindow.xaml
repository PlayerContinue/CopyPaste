<Window x:Class="CodePaste.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="clr-namespace:CodePaste.User_Controls"
        Title="MainWindow" Height="350" Width="525">

    <Grid Name="TempObject">
        <StackPanel VerticalAlignment="Top">
            <controls:menu x:Name="MainMenu">
            </controls:menu>
        </StackPanel>
        <controls:MainControl x:Name="MainController" CurrentControl="{Binding Path=CurrentView}" Margin="0,20,0,0">
        </controls:MainControl>
    </Grid>
</Window>